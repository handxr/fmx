{
  "name": "fmx",
  "version": "1.0.0",
  "description": "Turn Figma Make exports into clean React + Vite + Tailwind apps",
  "author": "juan hander",
  "license": "MIT",
  "type": "module",
  "bin": {
    "fmx": "./dist/cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "figma",
    "figma-make",
    "react",
    "vite",
    "tailwind",
    "code-generation"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "ink": "^5.2.0",
    "@inkjs/ui": "^2.0.0",
    "react": "^18.3.1",
    "meow": "^13.2.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "bun-types": "latest",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
